<NotepadPlus>
    <UserLang name="Markdown" ext="markdown mkdn mkd md" udlVersion="2.1">
        <Settings>
            <Global caseIgnored="yes" allowFoldOfComments="no" foldCompact="no" forcePureLC="0" decimalSeparator="0" />
            <Prefix Keywords1="yes" Keywords2="yes" Keywords3="yes" Keywords4="yes" Keywords5="no" Keywords6="no" Keywords7="no" Keywords8="no" />
        </Settings>
        <KeywordLists>
            <Keywords name="Comments">00# 00## 00### 00#### 00##### 01 02 03&lt;!-- 04--&gt;</Keywords>
            <Keywords name="Numbers, prefix1"></Keywords>
            <Keywords name="Numbers, prefix2"></Keywords>
            <Keywords name="Numbers, extras1"></Keywords>
            <Keywords name="Numbers, extras2"></Keywords>
            <Keywords name="Numbers, suffix1"></Keywords>
            <Keywords name="Numbers, suffix2"></Keywords>
            <Keywords name="Numbers, range"></Keywords>
            <Keywords name="Operators1">+</Keywords>
            <Keywords name="Operators2">-</Keywords>
            <Keywords name="Folders in code1, open"></Keywords>
            <Keywords name="Folders in code1, middle"></Keywords>
            <Keywords name="Folders in code1, close"></Keywords>
            <Keywords name="Folders in code2, open"></Keywords>
            <Keywords name="Folders in code2, middle"></Keywords>
            <Keywords name="Folders in code2, close"></Keywords>
            <Keywords name="Folders in comment, open"></Keywords>
            <Keywords name="Folders in comment, middle"></Keywords>
            <Keywords name="Folders in comment, close"></Keywords>
            <Keywords name="Keywords1">http:// (http:// https:// (https:// ftp:// (ftp:// git:// (git:// (/ ![ &lt;http</Keywords>
            <Keywords name="Keywords2">** __ * _</Keywords>
            <Keywords name="Keywords3">&gt;</Keywords>
            <Keywords name="Keywords4">--- ===</Keywords>
            <Keywords name="Keywords5">1. 2. 3. 4. 5. 6. 7. 8. 9. 0.</Keywords>
            <Keywords name="Keywords6"></Keywords>
            <Keywords name="Keywords7"></Keywords>
            <Keywords name="Keywords8"></Keywords>
            <Keywords name="Delimiters">00` 01 02` 03*** 04 05*** 06** 07 08** 09* 10 11* 12[ 13 14] 15&lt;http 16 17> 18&lt; 19 20> 21 22 23</Keywords>
        </KeywordLists>
        <Styles>
            <WordsStyle name="DEFAULT" fgColor="F8F8F8" bgColor="141414" fontName="" fontStyle="0" nesting="0" />
            <WordsStyle name="COMMENTS" fgColor="7587A6" bgColor="141414" fontName="" fontStyle="1" nesting="0" />
            <WordsStyle name="LINE COMMENTS" fgColor="9B703F" bgColor="141414" fontName="" fontStyle="1" nesting="9247" />
            <WordsStyle name="NUMBERS" fgColor="CF6A4C" bgColor="141414" fontName="" fontStyle="0" nesting="0" />
            <WordsStyle name="KEYWORDS1" fgColor="7587A6" bgColor="141414" fontName="" fontStyle="0" nesting="0" />
            <WordsStyle name="KEYWORDS2" fgColor="9B703F" bgColor="141414" fontName="" fontStyle="1" nesting="0" />
            <WordsStyle name="KEYWORDS3" fgColor="CF6A4C" bgColor="141414" fontName="" fontStyle="0" nesting="0" />
            <WordsStyle name="KEYWORDS4" fgColor="9B703F" bgColor="141414" fontName="" fontStyle="1" nesting="0" />
            <WordsStyle name="KEYWORDS5" fgColor="CF6A4C" bgColor="141414" fontName="" fontStyle="0" nesting="0" />
            <WordsStyle name="KEYWORDS6" fgColor="FFFFFF" bgColor="141414" fontName="" fontStyle="0" nesting="0" />
            <WordsStyle name="KEYWORDS7" fgColor="FFFFFF" bgColor="141414" fontName="" fontStyle="0" nesting="0" />
            <WordsStyle name="KEYWORDS8" fgColor="FFFFFF" bgColor="141414" fontName="" fontStyle="0" nesting="0" />
            <WordsStyle name="OPERATORS" fgColor="CF6A4C" bgColor="141414" fontName="" fontStyle="1" nesting="0" />
            <WordsStyle name="FOLDER IN CODE1" fgColor="F8F8F8" bgColor="141414" fontName="" fontStyle="0" nesting="0" />
            <WordsStyle name="FOLDER IN CODE2" fgColor="FFFFFF" bgColor="141414" fontName="" fontStyle="0" nesting="0" />
            <WordsStyle name="FOLDER IN COMMENT" fgColor="FFFFFF" bgColor="141414" fontName="" fontStyle="0" nesting="0" />
            <WordsStyle name="DELIMITERS1" fgColor="CDA869" bgColor="141414" fontName="Courier New" fontStyle="0" nesting="0" />
            <WordsStyle name="DELIMITERS2" fgColor="CF6A4C" bgColor="141414" fontName="" fontStyle="3" nesting="0" />
            <WordsStyle name="DELIMITERS3" fgColor="CF6A4C" bgColor="141414" fontName="" fontStyle="1" nesting="9243" />
